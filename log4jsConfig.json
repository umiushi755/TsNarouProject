{
  "appenders":{
    "console": {
      "type": "console",
      "level": "all",
      "layout": {
        "type": "pattern",
        "pattern": "%[[%d{yyyy/MM/dd hh:mm:ss.SSS}] [%p] %c -%] %m"
      }
    },
    "default":{
      "type": "file",
      "filename": "log/system.log",
      "layout": {
        "type": "pattern",
        "pattern": "[%d{yyyy/MM/dd hh:mm:ss.SSS}] [%p] %c - %m"
      },
      "maxLogSize": 1048576,
      "backups": 1,
      "category": "system"
    }
  },
  "categories": { 
    "default": { 
      "appenders": ["default","console"], 
      "level": "debug"
    }
  }
}